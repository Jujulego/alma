{
  "env": {
    "esm": {
      "presets": [
        ["@babel/preset-env", {
          "bugfixes": true,
          "modules": false
        }],
        ["@babel/preset-react", {
          "runtime": "automatic"
        }],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/proposal-class-properties"
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "bugfixes": true,
          "modules": "cjs"
        }],
        ["@babel/preset-react", {
          "runtime": "automatic"
        }],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/proposal-class-properties"
      ]
    }
  }
}