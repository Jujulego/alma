{
  "env": {
    "esm": {
      "presets": [
        ["@babel/preset-react", {
          "runtime": "automatic"
        }],
        "@babel/preset-typescript"
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "cjs",
          "targets": {
            "node": "14"
          }
        }],
        ["@babel/preset-react", {
          "runtime": "automatic"
        }],
        "@babel/preset-typescript"
      ]
    }
  }
}